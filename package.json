	"type": "module",
	"main": "./out/extension",
	"scripts": {
		"Document": "Document 'Source/**/*.ts'",
		"compile": "tsc -p ./",
		"prepublishOnly": "Build 'Source/**/*.ts'",
		"version": "tsc -v"
	},
	"contributes": {
		"commands": [
			{
				"command": "azure-iot-tools.showWelcomePage",
				"title": "Azure IoT Tools: Show Welcome Page"
			}
		]
	},
	"activationEvents": [
		"*",
		"onCommand:azure-iot-tools.showWelcomePage"
	],
	"dependencies": {
		"semver": "7.6.0"
	},
	"devDependencies": {
		"@playform/build": "0.0.7",
		"@playform/document": "0.0.6",
		"@types/node": "20.12.10",
		"@types/semver": "7.5.8"
	},
	"extensionPack": [
		"vsciot-vscode.azure-iot-edge",
		"vsciot-vscode.azure-iot-toolkit",
		"vsciot-vscode.vscode-iot-workbench"
	],
	"publishConfig": {
		"access": "public"
	},
	"icon": "azure-iot-tools-icon.png",
